<% if(home == "") { %>
    <div class="view jarallax" style="height: 70vh;">
        <img class="jarallax-img"
             src="https://www.sbtionline.com/wp-content/uploads/2017/10/woodcraft-1080x675.png"
             alt="home background" />
        <div class="mask rgba-black-light">
            <div class="container flex-center text-center">
                <div class="row mt-5">
                    <div class="col-md-12 text-white wow fadeIn mb-3">
                        <h1 class="display-3 mb-2 wow fadeInDown" data-wow-delay="0.3s">Company <a class="text-white-50 font-weight-bold">Name</a></h1>
                        <h2 class="text-uppercase h5 mb-3 mt-1 font-weight-bold wow fadeIn" data-wow-delay="0.4s">
                            Wood Crafting Merchant for Lawton Farmer's Market
                        </h2>
                        <a class="btn btn-brown btn-lg wow fadeIn" data-wow-delay="0.4s">portfolio</a> <a class="btn btn-outline-white btn-lg wow fadeIn" data-wow-delay="0.4s">About me</a>
                    </div>
                </div>
            </div>
        </div>
    </div>
<% } else { %>
    <% for(i =0; i < home.length; i++) { %>
        <div class="view jarallax" style="height: 70vh;">
            <% if(home.image == "") { %>
                <img class="jarallax-img"
                     src="https://www.sbtionline.com/wp-content/uploads/2017/10/woodcraft-1080x675.png"
                     alt="home background" />
            <% } else { %>
                    <img class="jarallax-img"
                         src="<%= home[i].image %>"
                         alt="<%= home[i].title %>" />
            <% } %>
            <div class="mask rgba-black-light">
                <div class="container flex-center text-center">
                    <div class="row mt-5">
                        <div class="col-md-12 text-white wow fadeIn mb-3">
                            <h1 class="display-3 mb-2 wow fadeInDown" data-wow-delay="0.3s"><%= home[i].title %></h1>
                            <h2 class="text-uppercase h5 mb-3 mt-1 font-weight-bold wow fadeIn" data-wow-delay="0.4s">
                                <%= home[i].subheading %>
                            </h2>
                            <a class="btn btn-brown btn-lg wow fadeIn" href="/projects" data-wow-delay="0.4s">portfolio</a>
                            <a class="btn btn-outline-white btn-lg wow fadeIn" href="#about" data-wow-delay="0.4s">About me</a>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    <% } %>
<% } %>